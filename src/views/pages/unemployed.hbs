<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Inactive Users</title>
  <link rel="stylesheet" href="/css/request.css" />
</head>
<body>
  <div class="request-container">
    <div class="top-container">
      <nav class="nav-bar">
        <div class="first-container">
          <div class="views">Inactive</div>
        </div>
      </nav>
    </div>

    <div class="content">
      <div class="nav-content">
        <h2>Inactive Users</h2>
      </div>

      <div class="table-container">
        <table>
          <thead>
            <tr>
              <th>Nombre</th>
              <th>Apellidos</th>
              <th>Correo Electr√≥nico</th>
              <th>Fecha Inicio</th>
              <th>Fecha Fin</th>
            </tr>
          </thead>
          <tbody>
            {{#if usuarios.length}}
              {{#each usuarios}}
              <tr>
                <td>{{Nombre}}</td>
                <td>{{Apellidos}}</td>
                <td>{{Correo_electronico}}</td>
                <td>{{formatDate Fecha_inicio_colab}}</td>
                <td>{{formatDate Fecha_vencimiento_colab}}</td>
              </tr>
              {{/each}}
            {{else}}
              <tr>
                <td colspan="5" class="empty-message">No hay usuarios inactivos</td>
              </tr>
            {{/if}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</body>
</html>
